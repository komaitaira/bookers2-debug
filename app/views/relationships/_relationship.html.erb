<% if current_user != user %>
    <% if user.followed_by?(current_user) %>
        <button><%=link_to "フォロー外す", user_relationships_path(user.id), method: :delete %></button>
    <% else %>
        <button><%=link_to "フォローする", user_relationships_path(user.id), method: :post %></button>
    <% end %>
<% end %>